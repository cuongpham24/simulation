    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "np.random.seed(2024)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "n = 10000\n",
    "U = np.random.uniform(0, 1, size=n)\n",
    "V = np.random.uniform(0, 1, size=n)\n",
    "Z = np.sqrt(-2 * np.log(U)) * np.cos(2 * np.pi * V)"
   ]
  },
